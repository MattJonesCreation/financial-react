{
    "extends": "react-app",
    "rules": {
        "no-multi-spaces": 2,
        "block-spacing": "error",
        "max-len":[1, { "code":160, "ignoreComments":true }],
        "no-var":"error",
        "no-irregular-whitespace": 2,
        "no-unexpected-multiline": 2,
        "curly": [2, "multi-line"], 
        "no-multi-str": 2,
        "no-unused-vars": [2, { "args": "none" }],
        "semi": 2,
        "semi-spacing": 2,
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}